<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="579.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/8.0.65"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.natebeckemeyer.projects.schedulrgui.graphics.MainWindowController">
    <children>
        <TableView fx:id="mainTaskList" layoutX="-1.0" layoutY="53.0" prefHeight="526.0" prefWidth="600.0">
            <columns>
                <TableColumn editable="false" prefWidth="99.0" text="Date"/>
                <TableColumn minWidth="0.0" prefWidth="500.0" text="Name"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <TextField fx:id="taskListDefinition" layoutX="97.0" layoutY="14.0" onKeyPressed="#enterTyping"
                   prefHeight="27.0" prefWidth="441.0" promptText="Define rule here!"/>
        <Button layoutX="545.0" layoutY="14.0" mnemonicParsing="false" onMousePressed="#provideView" text="View"/>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#viewAddTaskButtonClicked"
                text="Add Task"/>
    </children>
</AnchorPane>
